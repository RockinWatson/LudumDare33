            <kind>1</kind>
            <string>///----Step Event----//
//Get players Input
global.key_right = keyboard_check(ord('D'));
global.key_left = keyboard_check(ord('A'));
global.key_jump = keyboard_check_pressed(vk_space);
global.key_fire = mouse_check_button_pressed(mb_left);
global.key_crouch = keyboard_check_pressed(vk_shift);
global.key_stand = keyboard_check_released(vk_shift);


//----movement----//
move = -global.key_left + global.key_right;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;
//walking sound
if ((move or -move) and walking_sound == true){
    audio_play_sound(snd_run, 1, false);
    walking_sound = false;
    alarm[0] = 10;
} 
//stop running_sound if in Air
if (!place_meeting (x,y+1,obj_ground_collision) and audio_is_playing (snd_run)){
    audio_stop_sound(snd_run);    
}


//----Jump-----//
if (place_meeting(x,y+1,obj_ground_collision)) {
    if (jumps &lt;=1){
        audio_play_sound(snd_land, 10, false);
    }
       //----Crouching----//
    if (global.key_crouch){
        sprite_index = sprt_monster_crouch;
        jumps = 0;
        if (global.key_right or global.key_left){
            hsp = 0;
        }
    }
    if (global.key_stand){
        sprite_index = sprt_test_body;
    }
    jumps = jumpsmax;
}
//reset jump
if (global.key_jump) &amp;&amp; (jumps &gt; 0) {
    audio_play_sound(snd_jump, 1, false);
    jumps -=1;
    vsp = -jumpspeed;
}

//----horizontal collision-----//
if (place_meeting(x+hsp,y,obj_ground_collision)){
    while(!place_meeting(x+sign(hsp),y,obj_ground_collision)){
        x += sign(hsp);
    }
    hsp = 0;
    if (audio_is_playing(snd_run)){
        audio_stop_sound(snd_run);
    }
}
x += hsp;

//----vertical collision----//
if (place_meeting(x,y+vsp,obj_ground_collision)){
    while(!place_meeting(x,y+sign(vsp),obj_ground_collision)){
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;


//----Player Shooting----//
mouse_direction = point_direction(x,y,mouse_x,mouse_y);

if (global.key_fire and (mouse_direction &lt; 112.5 and mouse_direction &gt; 67.5))  {
    //shoot straight up
    bullet = instance_create(x,y-80, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction += 90;
} else if (global.key_fire and (mouse_direction &lt; 67.5 and mouse_direction &gt; 22.5)){
    //shoot at a 45 degree angle right
    bullet = instance_create(x,y, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction += 45;
} else if (global.key_fire and (mouse_direction &lt; 22.5 and mouse_direction &gt; 0)){
    //shoot right when mouse is inbetween 22.5 and 0
    bullet = instance_create(x,y, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction = 0;
} else if (global.key_fire and (mouse_direction &gt; 270 and mouse_direction &lt;= 359)){
    //shoot right when mouse is inbetween 270 and 359
    bullet = instance_create(x,y, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction = 0;
} else if (global.key_fire and (mouse_direction &lt; 270 and mouse_direction &gt; 157.5)){
    //shoot left when mouse is inbetween 270 and 157.5
    bullet = instance_create(x,y, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction = 180;
} else if (global.key_fire and (mouse_direction &lt; 157.5 and mouse_direction &gt; 112.5)){
    //shoot 45 angle left when mouse is inbetween 157.5 and 112.5
    bullet = instance_create(x,y, obj_bullet);
    audio_play_sound(snd_laser, 1, false);
    bullet.direction = 135;
}

</string>
